  "license": "MIT",
  "dependencies": {
    "build": "npm run minify -- --source-map reunderscore-min.map --source-map-url \" \" -o reunderscore-min.js",
    "chai": "^2.3.0",
    "eslint": "^3.12.2",
    "express": "^4.12.4",
    "minify": "uglifyjs reunderscore.js -c \"evaluate=false\" --comments \"/    .*/\" -m",
    "mocha": "^2.2.5",
    "uglify-js": "^2.7.5"
  }
}
